<!DOCTYPE html>
<html lang="es">

<head>
    <% include partials/helmet-bootstrap.ejs %>
</head>

<body>
    <% include partials/header-bootstrap-admin-in.ejs %>

    <section class="container-fluid">
        <br />
        <h2><strong>Administrar Restaurantes</strong></h2>
        <hr width="100%" noshade>
    </section>

    <section class="container-fluid">
        <button class="btn btn-primary bg-blue">+ Nuevo Restaurante</button>
        <br />
    </section>

    <section class="container-fluid">
        <div class="card-container row">
            <div class="d-flex align-items-stretch">
                <div class="card">
                    <img src="images/restaurants/1.jpg" class="card-img-top img-fluid imgResponsive"
